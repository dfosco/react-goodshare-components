{
  "name": "react-goodshare-components",
  "version": "0.1.0",
  "description": "React.js components for social share. A simple way to share a link on the pages of your website in the most popular (and not so) social networks. Powered by goodshare.js project.",
  "main": "dist/index.js",
  "proxy": "http://localhost:4000",
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "clearing": "rm -rf ./dist",
    "transpile": "babel src -d dist --copy-files --ignore **/**/*.mdx",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "build": "npm run clearing && npm run transpile && npm run docz:build && webpack --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koddr/react-goodshare-components.git"
  },
  "keywords": [
    "good share",
    "share button",
    "react share",
    "social share"
  ],
